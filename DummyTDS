---Ghost Gui UI Library
loadstring(game:HttpGet('https://raw.githubusercontent.com/GhostPlayer352/UI-Library/refs/heads/main/Ghost%20Gui'))()
game.CoreGui.GhostGui.MainFrame.Title.Text = "UT Troops"
---
 
 
 AddContent("TextButton", "Radio Mx", [[
local args = {
    [1] = "ScaleDummy",
    [2] = CFrame.new(game.Players.LocalPlayer.Character.Torso.Position),
}

game:GetService("ReplicatedStorage").Functions.SpawnTower:InvokeServer(unpack(args))

local args = {
    [1] = "Radio Dummy 5.0",
    [2] = CFrame.new(game.Players.LocalPlayer.Character.Torso.Position),
    [3] = workspace.Towers:FindFirstChild("ScaleDummy"),
    [5] = 0
}

game:GetService("ReplicatedStorage").Functions.SpawnTower:InvokeServer(unpack(args))

]])
 
