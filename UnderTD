local args = {
    [1] = true
}

game:GetService("ReplicatedStorage").PlayerRemoteEvents.Cuphead.CupheadSuperArt:FireServer(unpack(args))

]])

AddContent("TextButton", "Ungod", [[
local args = {
    [1] = false
}

game:GetService("ReplicatedStorage").PlayerRemoteEvents.Cuphead.CupheadSuperArt:FireServer(unpack(args))

]])

AddContent("TextButton", "Anti Monika", [[
game:GetService("ReplicatedStorage").PlayerRemoteEvents.MonikaSpringEffect:FireServer()

]])

