local function animateLoading()

    logoText:TweenPosition(UDim2.new(0.5, -250, 0.5, -120), "Out", "Quad", 1, true)

    for i = 1, 20 do

        logoText.TextTransparency = 1 - (i / 20)

        logoStroke.Transparency = 1 - (i / 40)

        logoStroke.Thickness = i / 4

        wait(0.04)

    end

    

    wait(0.3)

    

    for i = 1, 15 do

        subText.TextTransparency = 1 - (i / 15)

        versionText.TextTransparency = 1 - (i / 15)

        percentText.TextTransparency = 1 - (i / 15)

        statusText.TextTransparency = 1 - (i / 15)

        footerText.TextTransparency = 1 - (i / 15)

        wait(0.04)

    end

    

    wait(0.5)

    

    local states = {

        {percent = 15, text = "Cargando módulos..."},

        {percent = 35, text = "Inicializando sistemas..."},

        {percent = 55, text = "Configurando hitbox..."},

        {percent = 75, text = "Aplicando ajustes..."},

        {percent = 90, text = "Finalizando..."},

        {percent = 100, text = "Completado!"}

    }

    

    local currentPercent = 0

    for _, state in ipairs(states) do

        while currentPercent < state.percent do

            currentPercent = currentPercent + 1

            loadingBar:TweenSize(UDim2.new(currentPercent/100, 0, 1, 0), "Out", "Sine", 0.02, true)

            percentText.Text = currentPercent .. "%"

            wait(0.025)

        end

        statusText.Text = state.text

        wait(0.4)

    end

    

    wait(0.6)

    

    for i = 1, 5 do

        logoStroke.Thickness = 5 + i

        logoStroke.Transparency = 0.5

        wait(0.1)

        logoStroke.Transparency = 0.8

        wait(0.1)

    end

    

    wait(0.3)

    

    for i = 1, 15 do

        local trans = i / 15

        logoText.TextTransparency = trans

        subText.TextTransparency = trans

        versionText.TextTransparency = trans

        percentText.TextTransparency = trans

        statusText.TextTransparency = trans

        footerText.TextTransparency = trans

        barOuter.BackgroundTransparency = trans

        loadingBg.BackgroundTransparency = trans

        wait(0.03)

    end

    

    loadingGui:Destroy()

end

spawn(animateLoading)

wait(8)

