local showHitbox = false

local currentSize = 20

local originalSizes = {}

local function clamp(value, min, max)

    return math.max(min, math.min(max, value))

end

local function applyHitbox()

    for _, plr in pairs(game.Players:GetPlayers()) do

        if plr ~= player then

            local char = plr.Character

            if char then

                local hrp = char:FindFirstChild("HumanoidRootPart")

                if hrp then

                    if not originalSizes[plr.UserId] then

                        originalSizes[plr.UserId] = hrp.Size

                    end

                    

                    hrp.Size = Vector3.new(currentSize, currentSize, currentSize)

                    hrp.CanCollide = false

                    

                    if showHitbox then

                        hrp.Transparency = 0.95

                        hrp.BrickColor = BrickColor.new("White")

                        hrp.Material = Enum.Material.Neon

                    else

                        hrp.Transparency = 1

                        hrp.Material = Enum.Material.Plastic

                    end

                end

            end

        end

    end

end

toggleButton.MouseButton1Click:Connect(function()

    mainFrame.Visible = not mainFrame.Visible

    if mainFrame.Visible then

        toggleButton.BackgroundColor3 = Color3.fromRGB(40, 40, 40)

        mainFrame:TweenSize(UDim2.new(0, 260, 0, 180), "Out", "Quad", 0.2, true)

    else

        toggleButton.BackgroundColor3 = Color3.fromRGB(20, 20, 20)

    end

end)

sizeInput.FocusLost:Connect(function()

    local value = tonumber(sizeInput.Text) or 20

    currentSize = clamp(value, 1, 150)

    sizeInput.Text = tostring(currentSize)

end)

toggleViewBtn.MouseButton1Click:Connect(function()

    showHitbox = not showHitbox

    if showHitbox then

        toggleViewBtn.Text = "Ocultar"

        toggleViewBtn.BackgroundColor3 = Color3.fromRGB(50, 50, 50)

        toggleViewStroke.Color = Color3.fromRGB(100, 100, 100)

    else

        toggleViewBtn.Text = "Mostrar"

        toggleViewBtn.BackgroundColor3 = Color3.fromRGB(30, 30, 30)

        toggleViewStroke.Color = Color3.fromRGB(60, 60, 60)

    end

    applyHitbox()

end)

applyBtn.MouseButton1Click:Connect(function()

    applyHitbox()

    applyBtn.Text = "Aplicado!"

    applyBtn.BackgroundColor3 = Color3.fromRGB(60, 60, 60)

    applyStroke.Color = Color3.fromRGB(120, 120, 120)

    wait(1)

    applyBtn.Text = "Aplicar"

    applyBtn.BackgroundColor3 = Color3.fromRGB(40, 40, 40)

    applyStroke.Color = Color3.fromRGB(80, 80, 80)

end)

game.Players.PlayerAdded:Connect(function(plr)

    plr.CharacterAdded:Connect(function()

        wait(0.5)

        applyHitbox()

    end)

end)

for _, plr in pairs(game.Players:GetPlayers()) do

    plr.CharacterAdded:Connect(function()

        wait(0.5)

        applyHitbox()

    end)

end

local dragging = false

local dragInput

local dragStart

local startPos

title.InputBegan:Connect(function(input)

    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then

        dragging = true

        dragStart = input.Position

        startPos = mainFrame.Position

        

        input.Changed:Connect(function()

            if input.UserInputState == Enum.UserInputState.End then

                dragging = false

            end

        end)

    end

end)

title.InputChanged:Connect(function(input)

    if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then

        dragInput = input

    end

end)

game:GetService("UserInputService").InputChanged:Connect(function(input)

    if input == dragInput and dragging then

        local delta = input.Position - dragStart

        mainFrame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)

    end

end)
